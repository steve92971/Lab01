public class ch1 {

    public static void main(String[] args) {
        System.out.printf("f(-3.2) = %.4f\n",f(-3.2));//記得要f()和函數一樣
        System.out.printf("f(-2.1) = %.4f\n",f(-2.1));
        System.out.printf("f(0) = %.4f\n",f(0));
        System.out.printf("f(2.1) = %.4f\n",f(2.1));
    }
    static double f(double x) //函數的寫法
    {
        return 3*Math.pow(x,3)+2*x-1; //回傳用return
    }

}
